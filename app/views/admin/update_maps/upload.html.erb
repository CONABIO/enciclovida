<div class = "mt-5 mb-5">
  <div class = " row justify-content-center">
    <div class= "col-md-6">
      <h2> Importar archivos en excel para actualizacion de mapas en enciclovida</h2>
      <%= form_with url: admin_import_file_path, multipart: true, method: :post do |form| %>
        <br>
        <%= form.file_field :archivo_excel, class:"form-field"  %>
        <%= form.submit "Importar", class:"mt-5 btn btn-primary" %>
        <br>
      <% end %>
    </div>
  </div>
</div>