<div class="col-xs-12 col-sm-12 col-md-5 col-lg-4 media-supplier">
  <div class="col-xs-12 col-sm-6 col-md-12 col-lg-12">
    <i class ='cornell_lab-ev-icon'></i>
    <i class ='cornell_lab_colors-ev-icon'></i>
  </div>
  <div class="col-xs-12 col-sm-4 col-md-12 col-lg-12">
    <i class ='macaulay_library-ev-icon'></i>
  </div>
  <div class="col-xs-12 col-sm-2 col-md-12 col-lg-12">
    <span class='glyphicon glyphicon-volume-up' aria-hidden='true'></span>
  </div>
</div>

<% if @array[0][:msg].present? %>
  <div class='result-img-container'>
    <strong><%= @array[0][:msg]%></strong>
  </div>
<% else %>
	<div class="inner_media">
	  <% @array.each_with_index do |item, index|  %>
		  <div class="result-img-container cornell-audios">
		    <%= link_to("<img src='#{item['mlBaseDownloadUrl']}#{item['assetId']}/poster' />".html_safe, '', "data-toggle" => "modal", "data-target" => "#modal_reproduce", :class => "btn btn-link btn-title modal-buttons", "data-observation"=> item['citationUrl'], "data-url" => "#{item['mlBaseDownloadUrl']}/#{item['assetId']}/audio", "data-type" => 'audio', "data-author" => item['userDisplayName'], "data-date" => item['obsDtDisplay']||='', "data-country" => item['countryName']||='', "data-state" => item['subnational1Name']||='', "data-locality" => item['locality']||='') %>
		  </div>
		<% end %>
	</div>

	<nav aria-label="...">
	  <ul class="pager">
	    <li class="previous disabled"><a data-params="media-cornell?type=audio" data-direccion="-1"><span aria-hidden="true">&larr;</span> </a></li>
	    <li class="next"><a data-params="media-cornell?type=audio" data-direccion="1"> <span aria-hidden="true">&rarr;</span></a></li>
	  </ul>
	</nav>
<% end %>