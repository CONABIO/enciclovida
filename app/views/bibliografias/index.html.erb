<h1>Listing bibliografias</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bibliografias.each do |bibliografia| %>
      <tr>
        <td><%= link_to 'Show', bibliografia %></td>
        <td><%= link_to 'Edit', edit_bibliografia_path(bibliografia) %></td>
