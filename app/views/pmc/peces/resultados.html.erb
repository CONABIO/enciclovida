<div id="resultados-container" class="col-xs-12 state-two">
  <div class="row">
    <button id="toggleFiltros" class="btn btn-default pull-left" onclick="$('#resultados-container').toggleClass('state-one state-two');" type="button">
      <span class="glyphicon"></span>
    </button>
    <div class="pull-left filtrosUsados">
      <% fu = filtrosUsados.html_safe %>
      <% unless fu == '' %>
        <h5>
          <b>Tu búsqueda se realizó con los siguientes filtros: </b>
          <%= fu %>
        </h5>
      <% end %>
    </div>

  </div>
  <hr/>
    <%= render :file => 'pmc/peces/index' %>

  <div id="pestañas" class="panel">
    <ul class="nav nav-tabs" data-tabs="tabs">
      <li class="active">
        <a href="#todos" data-toggle="tab" id="nivel_" id_div="todos"><b><%= @peces.length %></b> Resultados</a>
      </li>
    </ul>

    <div class="tab-pane panel-body resultados-peces" id="todos">
        <div id="resultados">
          <%= render :file => 'pmc/peces/resultados_iteracion' %>
        </div>
    </div>
  </div>

</div>




