<div id="resultados-container" class="col-12 mb-3">
  <% fu = filtrosUsados.html_safe %>
  <% unless fu == '' %>
    <h6><b><%= fa_icon 'filter' %> La presente consulta utilizó los siguientes filtros: </b></h6>
    <!--h6><small>(especies en orden decreciente de recomendación)</small></h6-->
    <h6 class="filtrosUsados my-0 mx-3"><%= fu %></h6>
  <% end %>
</div>

<div class="col">
  <%= render :file => 'pmc/peces/index' %>
</div>

<div id="pestañas" class="col-9 panel">
  <ul class="nav nav-tabs" data-tabs="tabs">
    <li class="active">
      <a href="#todos" data-toggle="tab" id="nivel_" id_div="todos"><b><%= @peces.length %></b> Resultados</a>
    </li>
  </ul>

  <div class="tab-pane panel-body resultados-peces" id="todos">
    <div id="resultados" class="row">
      <%= render :file => 'pmc/peces/resultados_iteracion' %>
    </div>
  </div>
</div>

