<%= form_for :proy do |f| %>
  <%= f.text_field :institucion %>
  <%= f.select :tipo_monitoreo, Metamares::Proyecto::TIPO_MONITOREO.map{ |a| [t("metamares.proyectos.busqueda.#{a.first}", default: a.first), a.last] } %>
  <%= f.text_field :nombre_comun %>
  <%= f.hidden_field :nombre_cientifico %>
  <%= f.select :tipo_monitoreo, Usuario.all.collect {|u| [u.id, u.id]} %>
<% end %>