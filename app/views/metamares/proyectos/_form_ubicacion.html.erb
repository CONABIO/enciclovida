<%= f.semantic_fields_for :ubicacion do |f| %>

  <%= f.inputs :ubicacion_titulo do %>
    <%= f.input :calle_numero, :input_html => { :class => 'form-control'} %>
    <%= f.input :colonia, :input_html => { :class => 'form-control'} %>
    <%= f.input :municipio, :input_html => { :class => 'form-control'} %>
    <%= f.input :ciudad, :input_html => { :class => 'form-control'} %>
    <%= f.input :entidad_federativa, :input_html => { :class => 'form-control'} %>
    <%= f.input :cp, :input_html => { :class => 'form-control'} %>
    <%= f.input :pais, :input_html => { :class => 'form-control'} %>
  <% end %>

<% end %>