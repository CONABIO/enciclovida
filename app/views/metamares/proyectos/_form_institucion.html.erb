<%= f.semantic_fields_for :institucion do |f| %>

  <%= f.inputs :institucion_titulo do %>
    <%= f.input :nombre_institucion, :input_html => { :class => 'form-control'} %>
    <%= f.input :sitio_web, :input_html => { :class => 'form-control'} %>
    <%= f.input :contacto, :input_html => { :class => 'form-control'} %>
    <%= f.input :correo_contacto, :input_html => { :class => 'form-control'} %>
  <% end %>

<% end %>

<%= render partial: 'form_ubicacion', locals: { f: f } %>