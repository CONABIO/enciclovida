<%= f.semantic_fields_for :periodo do |f| %>

  <%= f.inputs :periodo_titulo do %>
    <%= f.input :periodicidad %>
    <%= f.input :periodo_monitoreo_desde %>
    <%= f.input :periodo_monitoreo_hasta %>
    <%= f.input :periodo_sistematico_desde %>
    <%= f.input :periodo_sistematico_hasta %>
    <%= f.input :monitoreo_desde %>
    <%= f.input :monitoreo_hasta %>
    <%= f.input :comentarios, as: :text %>
  <% end %>

<% end %>