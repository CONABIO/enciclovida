<% response.each do |m| %>
    <div class="panel panel-default">
      <b>From: </b><i><%= m.from.to_s %></i><br />
      <b>Subject: </b><i><%= m.subject.to_s %> - [<%= ['id'].to_s %> ]</i><br />
      <p class="class<%= ['id'].to_s %>"><b>Abstract: </b><i><%# v['fr'] %></i></p>
      <pre class="class<%# v['id']%>" mid="correoId<%# v['id'] %>" style="border-radius: 1.5em; margin: 15px;">
        <%= m.body.raw_source.html_safe %>
      </pre>
